{
  "name": "海豹母猪大家族",
  "version": "3.0.1",
  "manifest_version": 3,
  "description": "我以后只抽超得！https://coding.net/u/yoshinoshabby/p/boost8/git",
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "32": "icon.png"
    },
    "default_title": "boost8-master",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://gbf.game.mbga.jp/*",
		"http://game.granbluefantasy.jp/*"
      ],
      "js": [
        "bower_components/jquery/dist/jquery.js",
        "scripts/extra.js",
        "scripts/contentscript3.js"
	],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
		"notifications","browsingData","tabs" ,"http://localhost/","http://gbf.game.mbga.jp/*","storage","http://realtime.search.yahoo.co.jp/*"
  ],
  "web_accessible_resources": [
    "reload.js",
    "inject/click_.js",
    "inject/load_.js",
    "inject/engine.js",
    "inject/select_all_normal.js",
    "inject/weapon.js"
  ]
}
